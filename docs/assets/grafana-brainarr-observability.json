{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Latency p95 by provider:model",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "{__name__=~\"$latency_series\"}",
          "legendFormat": "{{__name__}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Errors by provider:model",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "{__name__=~\"$errors_series\"}",
          "legendFormat": "{{__name__}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "type": "timeseries",
      "title": "Throttles (429) by provider:model",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "targets": [
        {
          "expr": "{__name__=~\"$throttle_series\"}",
          "legendFormat": "{{__name__}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "type": "stat",
      "title": "p95 now (ms)",
      "gridPos": { "h": 4, "w": 12, "x": 0, "y": 17 },
      "targets": [
        { "expr": "max({__name__=~\"$latency_series\"})", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "center" }
    },
    {
      "type": "stat",
      "title": "p95 vs 24h baseline (%)",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 17 },
      "targets": [
        {
          "expr": "100 * (max({__name__=~\"$latency_series\"}) / clamp_min(max(avg_over_time({__name__=~\"$latency_series\"}[24h])), 1))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "center" }
    },
    {
      "type": "stat",
      "title": "Errors (current 1m)",
      "gridPos": { "h": 4, "w": 12, "x": 0, "y": 21 },
      "targets": [
        { "expr": "sum({__name__=~\"$errors_series\"})", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "center" }
    },
    {
      "type": "stat",
      "title": "429 (current 1m)",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 21 },
      "targets": [
        { "expr": "sum({__name__=~\"$throttle_series\"})", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "center" }
    },
    {
      "type": "timeseries",
      "title": "Latency p95 — ${provider}",
      "repeat": "provider",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 26 },
      "targets": [
        {
          "expr": "{__name__=~\"provider_latency_${provider:regex}_${model:regex}_p95\"}",
          "legendFormat": "{{__name__}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "type": "timeseries",
      "title": "Errors & 429 — ${provider}",
      "repeat": "provider",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 34 },
      "targets": [
        {
          "expr": "{__name__=~\"provider_errors_${provider:regex}_${model:regex}.*\"}",
          "legendFormat": "err: {{__name__}}",
          "refId": "A"
        },
        {
          "expr": "{__name__=~\"provider_429_${provider:regex}_${model:regex}.*\"}",
          "legendFormat": "429: {{__name__}}",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["brainarr", "observability"],
  "templating": {
    "list": [
      {
        "name": "provider",
        "type": "query",
        "label": "Provider",
        "datasource": "Prometheus",
        "includeAll": true,
        "allValue": ".+",
        "multi": true,
        "query": "label_values({__name__=~\"provider_latency_.*_p95\"}, __name__)",
        "regex": "/provider_latency_([^_]+)_.*/$1/",
        "refresh": 2
      },
      {
        "name": "model",
        "type": "query",
        "label": "Model",
        "datasource": "Prometheus",
        "includeAll": true,
        "allValue": ".+",
        "multi": true,
        "query": "label_values({__name__=~\"provider_latency_.*_p95\"}, __name__)",
        "regex": "/provider_latency_[^_]+_(.*)_p95/$1/",
        "refresh": 2
      },
      {
        "name": "latency_series",
        "type": "query",
        "label": "Latency series",
        "datasource": "Prometheus",
        "includeAll": true,
        "allValue": "provider_latency_.*_p95",
        "multi": true,
        "query": "label_values({__name__=~\"provider_latency_${provider:regex}_${model:regex}_p95\"}, __name__)",
        "refresh": 2
      },
      {
        "name": "errors_series",
        "type": "query",
        "label": "Error series",
        "datasource": "Prometheus",
        "includeAll": true,
        "allValue": "provider_errors.*",
        "multi": true,
        "query": "label_values({__name__=~\"provider_errors_${provider:regex}_${model:regex}.*\"}, __name__)",
        "refresh": 2
      },
      {
        "name": "throttle_series",
        "type": "query",
        "label": "429 series",
        "datasource": "Prometheus",
        "includeAll": true,
        "allValue": "provider_429.*",
        "multi": true,
        "query": "label_values({__name__=~\"provider_429_${provider:regex}_${model:regex}.*\"}, __name__)",
        "refresh": 2
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": { "refresh_intervals": ["15s", "30s", "1m", "5m"] },
  "timezone": "",
  "title": "Brainarr Observability (Preview)",
  "uid": null,
  "version": 1
}
